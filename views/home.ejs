                <!-- slider -->

                <div class="slider">
                    <div class="container">
                        <div class="wrapper">
                            <div class="carousel">
                                <div class="carousel-slides">
                                    <div class="carousel-slide active">
                                        <img src="assets/slider/slider0.jpg" alt="Image 1">
                                    </div>
                                    <div class="carousel-slide">
                                        <img src="assets/slider/slider1.jpg" alt="Image 2">
                                    </div>
                                    <div class="carousel-slide">
                                        <img src="assets/slider/slider2.jpg" alt="Image 3">
                                    </div>
                                </div>
                                <a class="carousel-control prev">&#10094;</a>
                                <a class="carousel-control next">&#10095;</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- slider -->

                <!-- trending products -->
                <div id="trending" class="trending">
                    <div class="container">
                        <div class="wrapper">
                            <div class="sectop flexitem">
                                <h2><span class="circle"></span><span>Trending Products</span></h2>
                                <div class="second-links">
                                    <a href="/trendingproducts" class="view-all">View all
                                        <i class="far fa-arrow-alt-circle-right"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="column">
                                <div class="flexwrap">
                                    <!-- sale products -->
                                    <div class="row products big">
                                        <div class="item">
                                            <div class="offer">
                                                <p>Offer ends at</p>
                                                <ul class="flexcenter">
                                                    <li>1</li>
                                                    <li>15</li>
                                                    <li>27</li>
                                                    <li>60</li>
                                                </ul>
                                            </div>
                                            <div class="media">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="images/apple/ma10.webp" alt="">
                                                    </a>
                                                </div>
                                                <div class="hoverable">
                                                    <ul>
                                                        <li class="active"><a href="#"><i
                                                                    class="fa-regular fa-heart"></i></a>
                                                        </li>
                                                        <li><a href="#"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                        </li>
                                                        <li><a href="#"><i class="fa fa-random"
                                                                    aria-hidden="true"></i></a>
                                                        </li>
                                                    </ul>
                                                </div>
                                                <!-- <div class="discount circle flexcenter"><span>
                                                                    
                                                                    </span></div> -->
                                            </div>
                                            <div class="content">
                                                <div class="rating">
                                                    <div class="stars"></div>
                                                    <span class="mini-text">308
                                                    </span>
                                                </div>
                                                <h3 class="main-links">
                                                    <a href="/ma10">
                                                        Apple iPhone 14 Pro Max (256GB, Deep Purple)
                                                    </a>
                                                </h3>
                                                <div class="price">
                                                    <span class="current">INR.137990.00
                                                    </span>
                                                    <span class="normal mini-text">INR.149900.00
                                                    </span>
                                                </div>
                                                <div class="stock mini-text">
                                                    <div class="qty">
                                                        <span>Stock: <strong class="qty-available">150
                                                            </strong></span>
                                                        <span>Sold: <strong class="qty-sold">73</strong></span>
                                                    </div>
                                                    <div class="bar">
                                                        <div class="available">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <% data.forEach((row)=> {%>
                                        <div class="row products mini">
                                            <div class="item">
                                                <div class="media">
                                                    <div class="thumbnail object-cover">
                                                        <a href="#">
                                                            <img src="<%= row.imageURL %>" alt="">
                                                        </a>
                                                    </div>
                                                    <div class="hoverable">
                                                        <ul>
                                                            <li class="active"><a href="#"><i
                                                                        class="fa-regular fa-heart"></i></a></li>
                                                            <li><a href="#"><i class="fa fa-eye"
                                                                        aria-hidden="true"></i></a></li>
                                                            <li><a href="#"><i class="fa fa-random"
                                                                        aria-hidden="true"></i></a></li>
                                                        </ul>
                                                    </div>
                                                    <!-- <div class="discount circle flexcenter"><span>32%</span></div> -->
                                                </div>
                                                <div class="content">
                                                    <h3 class="main-links"><a href="/<%= row.productcode %>">
                                                            <%= row.productname %>
                                                        </a></h3>
                                                    <div class="rating">
                                                        <div class="stars"></div>
                                                        <span class="mini-text">
                                                            <%= row.reviewed %>
                                                        </span>
                                                    </div>
                                                    <div class="price">
                                                        <span class="current">INR.<%= row.currentprice %></span>
                                                        <span class="normal mini-text">INR.<%= row.mrp %></span>
                                                    </div>
                                                    <div class="main-text">
                                                        <p>
                                                            <%= row.sold %> sold
                                                        </p>
                                                        <p>Free Shipping</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <% }); %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- trending products -->

                <!-- Featured products -->
                <div id="featuredproducts" class="features">
                    <div class="container">
                        <div class="wrapper">
                            <div class="column">
                                <div class="sectop flexitem">
                                    <h2><span class="circle"></span><span>Featured Products</span></h2>
                                    <div class="second-links">
                                        <a href="/featuredproducts" class="view-all">View all
                                            <i class="far fa-arrow-alt-circle-right"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="products main flexwrap">
                                    <% data.forEach((row)=> {%>
                                        <div class="item">
                                            <div class="media">
                                                <div class="image object-cover">
                                                    <a href="#">
                                                        <img src="<%= row.imageURL %>" alt="">
                                                    </a>
                                                </div>
                                                <div class="hoverable">
                                                    <ul>
                                                        <li class="active"><a href="#"><i
                                                                    class="fa-regular fa-heart"></i></a></li>
                                                        <li><a href="#"><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                        </li>
                                                        <li><a href="#"><i class="fa fa-random"
                                                                    aria-hidden="true"></i></a></li>
                                                    </ul>
                                                </div>
                                                <!-- <div class="discount circle flexcenter"><span>32%</span></div> -->
                                            </div>
                                            <div class="content">
                                                <h3 class="main-links"><a href="/<%= row.productcode %>">
                                                        <%= row.productname %>
                                                    </a>
                                                </h3>
                                                <div class="rating">
                                                    <div class="stars"></div>
                                                    <span class="mini-text">
                                                        <%= row.reviewed %>
                                                    </span>
                                                </div>
                                                <div class="price">
                                                    <span class="current">INR.<%= row.currentprice %></span>
                                                    <span class="normal mini-text">INR.<%= row.mrp %></span>
                                                </div>
                                                <div class="footer">
                                                    <ul class="mini-text">
                                                        <%= row.features %>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                        <% }); %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Featured products -->

                <!-- banners -->

            </main>
            <%- include('includes/footer'); %>
        </div>
        
    <script src="javascript/script.js"></script>
    </body>